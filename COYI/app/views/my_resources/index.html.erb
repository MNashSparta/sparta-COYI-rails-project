<h1>My Resources</h1>
<p id="notice"><%= notice %></p>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>User</th>
      <th>Chapter</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @my_resources.each do |my_resource| %>
      <tr>
        <td><%= my_resource.title %></td>
        <td><%= my_resource.description %></td>
        <td><%= User.find(my_resource.user_id).email %></td>
        <td><%= User.find(my_resource.user_id).location %></td>
        <td><%= link_to 'Show', my_resource %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%- if user_signed_in? %>
  <%= link_to "Add Resource's", new_my_resource_path %> |
  <%= link_to 'My Resources', user_my_resources_path %>
<% else %>
<% end %>
</div>
