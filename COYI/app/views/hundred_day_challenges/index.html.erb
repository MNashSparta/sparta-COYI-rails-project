<p id="notice"><%= notice %></p>

<h1>Hundred Day Challenges</h1>

<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>Address</th>
      <th>First name</th>
      <th>Last name</th>
      <th>Location</th>
      <th>Organisation</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @hundred_day_challenges.each do |hundred_day_challenge| %>
      <tr>
        <td><%= hundred_day_challenge.Email %></td>
        <td><%= hundred_day_challenge.Address %></td>
        <td><%= hundred_day_challenge.First_name %></td>
        <td><%= hundred_day_challenge.Last_name %></td>
        <td><%= hundred_day_challenge.Location %></td>
        <td><%= hundred_day_challenge.Organisation %></td>
        <td><%= link_to 'Show', hundred_day_challenge %></td>
        <td><%= link_to 'Edit', edit_hundred_day_challenge_path(hundred_day_challenge) %></td>
        <td><%= link_to 'Destroy', hundred_day_challenge, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Hundred Day Challenge', new_hundred_day_challenge_path %>
