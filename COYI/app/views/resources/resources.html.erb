<div class="col-md-3 first-column">
  <ul class="docs">
    <!-- We need a line or some small indication of which resources come from which level for higher levels -->
    <!-- Level 1 indication -->
  <% if policy(:myresources).level_1? %>
    <%= render "resources/resourceslevel1" %>
    <!-- Level 2 indication -->
  <%elsif policy(:myresources).level_2? || policy(:myresources).level_5?%>
    <%= render "resources/resourceslevel1" %>
    <!-- level 3 and up -->
    <%= render "resources/resourceslevel2-4" %>
  <%end%>
  </ul>
</div>
<div class="col-md-3 second-column">
  <ul class="docs">
    <!-- Level 5 indication -->
  <% if policy(:myresources).level_5? %>
    <%= render "resources/resourceslevel5" %>
  <%end%>
  </ul>
</div>

