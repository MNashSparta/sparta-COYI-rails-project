<link rel="stylesheet" href="/Stylesheets/news.css">
<p id="notice"><%= notice %></p>
<h1>News</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Story</th>
      <th>Published</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @news.each do |news| %>
      <tr>
        <td><%= news.title %></td>
        <td><%= news.story %></td>
        <td><%= news.published %></td>
        <td><%= link_to 'Show', news %></td>
        <!-- <td><%= link_to 'Edit', edit_news_path(news) %></td> -->
        <!-- <td><%= link_to 'Destroy', news, method: :delete, data: { confirm: 'Are you sure?' } %></td> -->
      </tr>
    <% end %>
  </tbody>
</table>

<br>

